{
    "kind": "template",
    "name": "oktadev:oag_diskInspectorWidget",
    "target": "oktadev:oag_disk",
    "view": "default",
    "element": {
        "instanceOf": "elements",
        "elements": {
            "instanceOf": {
                "name": "inspectorWidget"
            },
            "elements": {
                "instanceOf": {
                    "name": "properties"
                },
                "elements": [
                    {
                        "label": {
                            "instanceOf": "text",
                            "content": "Host Name"
                        },
                        "value": {
                            "instanceOf": "string",
                            "path": "attributes(host_name)"
                        }
                    },
                    {
                        "label": {
                            "instanceOf": "text",
                            "content": "IP Address"
                        },
                        "value": {
                            "instanceOf": "string",
                            "path": "attributes(ip_address)"
                        }
                    },
                    {
                        "label": {
                            "instanceOf": "text",
                            "content": "Device Path"
                        },
                        "value": {
                            "instanceOf": "string",
                            "path": "attributes(device_path)"
                        }
                    },
                    {
                        "label": {
                            "instanceOf": "text",
                            "content": "File System Type"
                        },
                        "value": {
                            "instanceOf": "string",
                            "path": "attributes(file_sys_type)"
                        }
                    },
                    {
                        "label": {
                            "instanceOf": "text",
                            "content": "Mountpoint"
                        },
                        "value": {
                            "instanceOf": "string",
                            "path": "attributes(mountpoint)"
                        }
                    }
                ]
            },
            "title": "Properties"
        }
    }
}
